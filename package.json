{
  "name": "star-chef",
  "version": "1.0.0",
  "description": "A killer site to turn you into a star chef",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && ava test/**/*tests.js --verbose",
    "build": "browserify -t [ babelify --presets [ react ] ] src/index.js -o build/app.js && cp -r src/api build && cp -r src/styles build",
    "start": "simplehttpserver"
  },
  "author": "Will Munn",
  "license": "ISC",
  "dependencies": {
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-preset-env": "^1.1.8",
    "chromedriver": "2.25.1",
    "selenium-webdriver": "3.0.1",
    "simplehttpserver": "^0.0.6"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "react"
      ]
    }
  }
}
